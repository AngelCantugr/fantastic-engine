    ]
  },
  "sources": {
    "preferred": [
      "techcrunch.com",
      "arstechnica.com",
      "theverge.com",
      "hacker news",
      "github blog"
    ],
    "technical": [
      "arxiv.org",
      "research.anthropic.com",
      "openai.com/research"
