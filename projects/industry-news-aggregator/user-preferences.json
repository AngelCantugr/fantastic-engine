  "adhd_optimizations": {
    "use_bullet_points": true,
    "highlight_key_takeaways": true,
    "include_tldr": true,
    "visual_separators": true,
    "emoji_markers": true
  },
  "storage": {
    "auto_cleanup_days": 90,
    "compress_old_raw_data": true,
    "max_storage_mb": 500
  }
